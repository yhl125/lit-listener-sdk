{
  "name": "circuit-base",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/circuit-base/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/circuit-base",
        "main": "packages/circuit-base/src/index.ts",
        "tsConfig": "packages/circuit-base/tsconfig.lib.json",
        "assets": ["packages/circuit-base/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/circuit-base/**/*.ts",
          "packages/circuit-base/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/circuit-base/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "deploy": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    }
  },
  "tags": []
}
